<?xml version="1.0" ?><xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 http://docs.oasis-open.org/xliff/v1.2/cs02/xliff-core-1.2-strict.xsd">
    <file datatype="plaintext" original="languages/en/tl_calendar.xlf" source-language="en" target-language="de">
        <body>
            <trans-unit id="tl_calendar.master.0">
                <source>Master calendar</source>
                <target>Hauptkalender</target>
            </trans-unit>
            <trans-unit id="tl_calendar.master.1">
                <source>Please define the master calendar to allow language switching.</source>
                <target>Wählen Sie den Hauptkalender um Sprachenwechel zu ermöglichen.</target>
            </trans-unit>
            <trans-unit id="tl_calendar.master.2">
                <source>This master has already been selected for the &quot;%s&quot; calendar!</source>
                <target>Dies ist bereits der Hauptkalender für &quot;%s&quot;!</target>
            </trans-unit>
            <trans-unit id="tl_calendar.language.0">
                <source>Language</source>
                <target>Sprache</target>
            </trans-unit>
            <trans-unit id="tl_calendar.language.1">
                <source>Please enter the page language according to the ISO-639-1 standard (e.g. &quot;en&quot; for English or &quot;en-US&quot; for American English).</source>
                <target>Bitte geben Sie die Sprache gemäss ISO-639-1 Standard ein (z.B. &quot;en&quot; für Englisch oder &quot;en-US&quot; für amerikanisches Englisch).</target>
            </trans-unit>
            <trans-unit id="tl_calendar.language_legend">
                <source>Language settings</source>
                <target>Spracheinstellungen</target>
            </trans-unit>
            <trans-unit id="tl_calendar.isMaster">
                <source>This is a master calendar</source>
                <target>Dies ist ein Hauptkalender</target>
            </trans-unit>
            <trans-unit id="tl_calendar.isSlave">
                <source>Master calendar is &quot;%s&quot;</source>
                <target>Der Hauptkalender ist &quot;%s&quot;</target>
            </trans-unit>
        </body>
    </file>
</xliff>