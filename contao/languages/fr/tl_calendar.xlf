<?xml version="1.0" ?><xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 http://docs.oasis-open.org/xliff/v1.2/cs02/xliff-core-1.2-strict.xsd">
    <file datatype="plaintext" original="languages/en/tl_calendar.xlf" source-language="en" target-language="fr">
        <body>
            <trans-unit id="tl_calendar.master.0">
                <source>Master calendar</source>
                <target>Calendrier de base</target>
            </trans-unit>
            <trans-unit id="tl_calendar.master.1">
                <source>Please define the master calendar to allow language switching.</source>
                <target>Merci de sélectionner le calendrier de base pour permettre le changement de langue.</target>
            </trans-unit>
            <trans-unit id="tl_calendar.master.2">
                <source>This master has already been selected for the &quot;%s&quot; calendar!</source>
            </trans-unit>
            <trans-unit id="tl_calendar.language.0">
                <source>Language</source>
                <target>Langue</target>
            </trans-unit>
            <trans-unit id="tl_calendar.language.1">
                <source>Please enter the page language according to the ISO-639-1 standard (e.g. &quot;en&quot; for English or &quot;en-US&quot; for American English).</source>
                <target>Merci d'introduire la langue de la page conformément à la norme ISO-639-1 (par exemple &quot;en&quot; pour l'anglais  ou &quot;en-US&quot; pour l'anglais américain).</target>
            </trans-unit>
            <trans-unit id="tl_calendar.language_legend">
                <source>Language settings</source>
                <target>Paramètres de la langue</target>
            </trans-unit>
            <trans-unit id="tl_calendar.isMaster">
                <source>This is a master calendar</source>
                <target>Il s'agit d'un calendrier de base</target>
            </trans-unit>
            <trans-unit id="tl_calendar.isSlave">
                <source>Master calendar is &quot;%s&quot;</source>
                <target>Le calendrier de base est &quot;%s&quot;</target>
            </trans-unit>
        </body>
    </file>
