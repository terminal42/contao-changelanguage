<?xml version="1.0" ?><xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 http://docs.oasis-open.org/xliff/v1.2/cs02/xliff-core-1.2-strict.xsd">
    <file datatype="plaintext" original="languages/en/tl_page.xlf" source-language="en" target-language="pl">
        <body>
            <trans-unit id="tl_page.languageMain.0">
                <source>Main language page</source>
                <target>Strona głównego języka</target>
            </trans-unit>
            <trans-unit id="tl_page.languageMain.1">
                <source>Choose the page in the language marked as fallback equals this page.</source>
                <target>Wybierz stronę w języku oznaczonym jako domyślny odpowiadającą tej stronie.</target>
            </trans-unit>
            <trans-unit id="tl_page.languageRoot.0">
                <source>Foreign-Domain Root Page</source>
                <target>Punkt startowy innej domeny</target>
            </trans-unit>
            <trans-unit id="tl_page.languageRoot.1">
                <source>Select the root page if this page tree is related to another domain.</source>
                <target>Wybierz punkt startowy, jeśli to drzewo jest powiązane z inną domeną.</target>
            </trans-unit>
            <trans-unit id="tl_page.languageRoot.2">
                <source>This is a main language root site</source>
                <target>To jest strona głównego języka</target>
            </trans-unit>
            <trans-unit id="tl_page.languageQuery.0">
                <source>Keep query parameters</source>
                <target>Zachowaj parametry URL</target>
            </trans-unit>
            <trans-unit id="tl_page.languageQuery.1">
                <source>Enter a comma separated list of query parameters that should be kept when switching to a translation of this page.</source>
                <target>Wprowadź oddzieloną przecinkami listę parametrów URL, który powinny zostać zachowane przy zmianie tłumaczenia tej strony.</target>
            </trans-unit>
            <trans-unit id="tl_page.language_legend">
